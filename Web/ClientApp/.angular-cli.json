{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"project": { "name": "web" },
	"apps": [
		{
			"root": "src",
			"outDir": "dist",
			"index": "index.html",
			"main": "main.ts",
			"polyfills": "polyfills.ts",
			"tsconfig": "tsconfig.app.json",
			"prefix": "app",
			"styles": ["styles.scss"],
			"environmentSource": "environments/environment.ts",
			"environments": { "development": "environments/environment.ts" }
		}
	],
	"defaults": {
		"styleExt": "scss",
		"build": { "progress": true }
	},
	"warnings": { "typescriptMismatch": false }
}
